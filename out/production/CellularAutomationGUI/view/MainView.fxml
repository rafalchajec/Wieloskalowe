<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane minHeight="600.0" minWidth="800.0" prefHeight="650.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MainController">
    <children>
        <TabPane prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="0.0"
                 AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <tabs>
                <Tab text="Cellular Automation 1D">
                    <content>
                        <fx:include fx:id="View1D" source="tabs/View1D.fxml"/>
                    </content>
                </Tab>
                <Tab text="GameOfLife">
                    <content>
                        <fx:include fx:id="ViewGameOfLife" source="tabs/ViewGameOfLife.fxml"/>
                    </content>
                </Tab>
                <Tab text="GrainGrowth">
                    <content>
                        <fx:include fx:id="ViewGrainGrowth" source="tabs/ViewGrainGrowth.fxml"/>
                    </content>
                </Tab>
            </tabs>
        </TabPane>
    </children>
</AnchorPane>
